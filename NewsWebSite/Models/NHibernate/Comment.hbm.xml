<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="NewsUa" namespace="NewsUa.Models">
  <class name="Comment" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" type="int">
      <generator class="native" />
    </id>
    <property name="UserName" />
    <property name="UserId" />
    <property name="Text" />
    <property name="ArticleId" />
    <property name="Depth" />
    <property name="ReplyCommentId" />
    <property name="Created" />
    <property name="Deleted" />
    <property name="UserImage" />
  </class>
</hibernate-mapping>